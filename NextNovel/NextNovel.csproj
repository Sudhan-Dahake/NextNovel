<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\Account.jpg" />
    <None Remove="Images\Book.jpg" />
    <None Remove="Images\Heart.jpg" />
    <None Remove="Images\Home.jpg" />
    <None Remove="Images\Image1.jpg" />
    <None Remove="Images\Logo.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Account.jpg" />
    <Resource Include="Images\Book.jpg" />
    <Resource Include="Images\Heart.jpg" />
    <Resource Include="Images\Home.jpg" />
    <Resource Include="Images\Image1.jpg" />
    <Resource Include="Images\Logo.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
